{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./src/compiledTS",
        "allowJs": true,
        "lib": ["DOM", "ES6"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
}
